{
    "name": "codemagpie/ddd-contracts",
    "type": "library",
    "description": "A domain driven design contracts",
    "keywords": [
        "ddd",
        "ddd-contracts"
    ],
    "authors": [
        {
            "name": "lyf",
            "email": "1191628562@qq.com"
        }
    ],
    "require": {
        "php": ">=7.4",
        "codemagpie/array2object": "^1.0",
        "codemagpie/simple-query-builder": "^1.2"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.1",
        "phpunit/phpunit": "^9.5"
    },
    "license": "MIT",
    "scripts": {
        "cs-fix": "php-cs-fixer fix $1"
    },
    "archive": {
        "exclude": ["/tests", ".gitattributes", ".gitignore", ".php-cs-fixer.php"]
    },
    "prefer-stable": true,
    "config": {
        "secure-http": false
    },
    "autoload": {
        "psr-4": {
            "CodeMagpie\\DDDContracts\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "CodeMagpie\\DDDContractsTest\\": "tests/"
        }
    }
}
